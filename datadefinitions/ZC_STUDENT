      Age,
  @EndUserText.label: 'Course'
      Course,
  @EndUserText.label: 'Course Duration'
      Courseduration,
  @EndUserText.label: 'Status'
      Status,
  @EndUserText.label: 'Gender'
      Gender,
  @EndUserText.label: 'DOB'
      Dob
}

