define table zstudent {

  key client     : abap.clnt not null;
  key id         : sysuuid_x16 not null;
  firstname      : abap.char(100);
  lastname       : abap.char(100);
  age            : abap.numc(4);
  course         : abap.char(50);
  courseduration : abap.numc(4);
  status         : abap_boolean;
